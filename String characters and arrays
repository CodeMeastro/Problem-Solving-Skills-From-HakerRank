#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n=0;string str="";
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>str;
        for(int j=0;j<str.length();j++){
            if(j%2==0){
                cout<<str[j];
            }
        }
        cout<<" ";
        for(int j=0;j<str.length();j++){
            if(j%2!=0){
                cout<<str[j];
            }
        }
        cout<<endl;
    }
    return 0;
}
